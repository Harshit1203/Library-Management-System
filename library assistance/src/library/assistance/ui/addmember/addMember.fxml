<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="301.0" prefWidth="530.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.assistance.ui.addmember.AddMemberController">
    <stylesheets>
        <URL value="@addmember.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="301.0" prefWidth="530.0">
         <children>
            <TextField fx:id="name" opacity="0.7" promptText="NAME" stylesheets="@addmember.css">
               <VBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="40.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </TextField>
            <TextField fx:id="id" opacity="0.7" promptText="ID" stylesheets="@addmember.css">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
               <effect>
                  <Blend />
               </effect>
            </TextField>
            <TextField fx:id="email" opacity="0.7" promptText="EMAIL" stylesheets="@addmember.css">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
               <effect>
                  <Blend />
               </effect>
            </TextField>
            <TextField fx:id="mobile" opacity="0.7" promptText="MOBILE NO." stylesheets="@addmember.css">
               <VBox.margin>
                  <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
               <effect>
                  <Blend />
               </effect>
            </TextField>
            <HBox prefHeight="86.0" prefWidth="513.0">
               <children>
                  <Button fx:id="savebutton" mnemonicParsing="false" onAction="#addMember" prefHeight="80.0" prefWidth="275.0" styleClass="save" stylesheets="@addmember.css" text="Save" textAlignment="CENTER" />
                  <Button fx:id="cancelbutton" mnemonicParsing="false" onAction="#cancel" prefHeight="85.0" prefWidth="263.0" styleClass="cancel" stylesheets="@addmember.css" text="Cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
